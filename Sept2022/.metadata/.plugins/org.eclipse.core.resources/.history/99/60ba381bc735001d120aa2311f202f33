package com.saran.app;

public class Second {
	int id;
	String name;
	
	public static void main(String[] args) {
		Second s1 = new Second();
		Second s2 = new Second();
		s1.id = 10;
		s1.name = "Jethon";
		s2.id = 11;
		s2.name = "Adam";
		
		System.out.println(s1.id + " " + s1.name);
		System.out.println(s2.id + " " + s2.name);
	}
}
